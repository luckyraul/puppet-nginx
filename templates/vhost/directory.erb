
  root <%= @root_folder %>;
<% if @index_files and @index_files.count > 0 -%>
  index <% Array(@index_files).each do |i| %> <%= i %><% end %>;
<% end -%>
<%- if @charset -%>
  charset <%= @charset %>;
<%- end -%>
<%- if @autoindex -%>
  autoindex <%= @autoindex %>;
<%- end -%>
<% if @headers -%>
  <%- @headers.each do |i| -%>
  add_header <%= i %>;
  <%- end -%>
<% end -%>
